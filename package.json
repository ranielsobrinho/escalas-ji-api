{
  "name": "escalas-ji-api",
  "version": "0.3.1",
  "description": "An api made with clean architecture and TDD, design patterns and more.",
  "main": "index.js",
  "scripts": {
    "start": "sucrase-node src/main/server.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/main/server.ts",
    "prepare": "husky install",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:ci": "npm test -- --coverage",
    "prettier": "npx prettier --config .prettierrc --write \"src/**/*.ts\""
  },
  "keywords": [
    "Typescript",
    "Nodejs",
    "API",
    "Javascript"
  ],
  "author": "Raniel Sobrinho",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.25",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "fast-glob": "^3.2.11",
    "git-commit-msg-linter": "^4.1.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.8",
    "prettier": "^2.6.2",
    "prisma": "^4.1.1",
    "sucrase": "^3.24.0",
    "supertest": "^6.2.4",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.2.3"
  },
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "@prisma/client": "^4.1.1",
    "bcrypt": "^5.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "validator": "^13.7.0"
  }
}
